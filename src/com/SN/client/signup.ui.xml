<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	.dem{
	padding-left:33%;padding-top:8%;

	}
 .qv{
width:100%;

height:100%;
 }
 	</ui:style>
	<g:HTMLPanel> 
	<m:MaterialPanel containerEnabled="true"/>
	<m:MaterialRow addStyleNames="{style.dem}">
	<m:MaterialColumn grid="s12 m8 l6" offset="16" >
	<m:MaterialTextBox ui:field="t1"  type="TEXT" label="Name" iconType="ACCOUNT_CIRCLE"/>
	<m:MaterialTextBox ui:field="t2"  type="EMAIL" label="Email" iconType="EMAIL"/>
	<m:MaterialTextBox ui:field="t3"  type="PASSWORD" label="Password" iconType="LOCK" />
<m:MaterialTextBox ui:field="t4" type="TEXT" label="Phone Number" iconType="PHONE" />
<m:MaterialButton ui:field="b1" waves="LIGHT" text="Sign Up" width="100%"/>
</m:MaterialColumn>
</m:MaterialRow>
</g:HTMLPanel>
</ui:UiBinder> 